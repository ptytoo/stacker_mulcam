<!DOCTYPE html>
<html>
<head>
  <title>개인회원 페이지</title>
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
  <!-- Load D3.js -->
  <script src="https://d3js.org/d3.v4.min.js"></script>
  <!-- Font Awesome -->
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css">
  <!-- Bootstrap core CSS -->
  <link href="<%= asset_path 'bootstrap.min.css'%>" rel="stylesheet">
  <!-- Material Design Bootstrap -->
  <link href="<%= asset_path 'mdb.css'%>" rel="stylesheet">
  <!-- Your custom styles (optional) -->
  <link href="<%= asset_path 'style.css'%>" rel="stylesheet">

  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
  <script src="../assets/billboard.js"></script>
  <link rel="../assets/stylesheet" href="billboard.css">
</head>
<body>

<%= render '/layouts/nav'%>

<%= yield %>
<%= render '/layouts/footer'%>

<!-- SCRIPTS -->
<!-- JQuery -->
<script type="text/javascript" src="<%= asset_path 'jquery-3.2.1.min.js'%>"></script>
<!-- Bootstrap tooltips -->
<script type="text/javascript" src="<%= asset_path 'popper.min.js'%>"></script>
<!-- Bootstrap core JavaScript -->
<script type="text/javascript" src="<%= asset_path 'bootstrap.min.js'%>"></script>
<!-- MDB core JavaScript -->
<script type="text/javascript" src="<%= asset_path 'mdb.min.js'%>"></script>

</body>
</html>
